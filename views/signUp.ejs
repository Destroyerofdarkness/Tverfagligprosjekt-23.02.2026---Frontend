<%- include("./partials/head.ejs") %>
<link rel="stylesheet" href="/css/auth.css">
<script src="/script/fetch_signUp.js" defer></script>
<%- include("./partials/header.ejs") %>

<form>
    <section>
    <label for="username">Brukernavn:</label>
    <input type="text" name="username" id="username" placeholder="Skriv inn brukernavnet..">
    <div class="error user"></div>
    </section>
    <section>
    <label for="passwd">Passord:</label>
    <input type="password" name="passwd" id="passwd" placeholder="Skriv inn passordet..">
    </section>
    <section>
    <label for="conPass">Gjenta passord:</label>
    <input type="password" name="conPass" id="conPass" placeholder="Gjenta passordet..">
    <div class="error passwd"></div>
    </section>
    <a href="/loggInn">Har allerede en konto?</a>
    <button type="submit">Registrer</button>
</form>

    
</body>
</html>